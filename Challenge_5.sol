pragma solidity ^0.8.17;

contract Function 
{
        uint a ;
        uint b ;
        uint sum ;
    function add()  public
    {
    
    sum = a+b;

    }

    function sub()  public 
    {
    sum = a-b;
    }  
    
      function mul()  public 
    {
    sum = a*b;
    }   
    
     function div()  public 
    {
    sum = a/b;
    }
}

